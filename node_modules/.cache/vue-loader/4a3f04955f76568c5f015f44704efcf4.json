email\"\n        required\n      />\n    </div>\n    <div class=\"brief\">\n      <label for=\"brief\">Brief</label>\n      <textarea id=\"brief\"></textarea>\n    </div>\n    <div class=\"phone\">\n      <label for=\"phone\">Phone</label>\n      <input\n        type=\"number\"\n        placeholder=\"Type Your Phone Number\"\n        id=\"phone\"\n        required\n      />\n    </div>\n    <a class=\"btn\" href=\"https://elzero.org/\" target=\"_blank\"> Register </a>\n  </div>\n</template>\n\n<script>\n// import { mapActions } from \"vuex\";\nexport default {\n  name: \"Signup\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n    };\n  },\n  methods: {\n    signup(email, password) {\n      console.log(\"we made it\");\n      fetch(\"https://ecommerce-pr.herokuapp.com/users/signup\", {\n        method: \"POST\",\n        mode: \"cors\",\n        body: JSON.stringify({\n          email: email,\n          password: password,\n        }),\n      })\n        .then((data) => data.json())\n        .then((user) => {\n          console.log(user);\n        });\n    },\n    // ...mapActions(\"account\", [\"login\"]),\n  },\n};\n</script>\n\n<style>\n.form-control {\n  border-radius: 0%;\n  height: 50px;\n}\n.login-form-container {\n  padding: 20px;\n  box-shadow: 0px 2px 5px 2px #888888;\n}\n.btn {\n  border-radius: 0%;\n  font-weight: bold;\n  background: teal;\n  border: teal;\n}\n.btn:hover {\n  background: #00b4b4;\n}\n.btn:focus {\n  background: teal;\n}\n</style>"]}]}